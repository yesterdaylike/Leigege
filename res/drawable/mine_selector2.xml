<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<!-- 触摸模式下单击时的背景图片-->
    <item 
          android:state_pressed="true"
          android:drawable="@drawable/mine_press2" />

	<!--获得焦点时的图片背景-->
    <item android:state_focused="true"
          android:drawable="@drawable/mine_longpress2" />
    
    <!-- 默认时的背景图片-->
    <item android:drawable="@drawable/mine_normal2" />

</selector>